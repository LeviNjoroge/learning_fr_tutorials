
void main(){
  Person joe = Person();
  joe.name = "Joseph"
}

class Person{
  String? name, sex;
  int? age;

  void createAttributes(String name,String sex,int age){
    this.name = name;
    this.sex = sex;
    this.age = age;
    return 1;
  }

  String displayData(String race){
    return "Name: ${name}\nSex: ${sex}\nAge: ${age} is ${race}";
  }
}